{
  "compilerOptions": {
      "module": "commonjs",
      "forceConsistentCasingInFileNames": true,
      "target": "ES5",
      "noImplicitAny": true,
      "alwaysStrict": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "sourceMap": true,
      "allowJs": false,
      "jsx": "react",
      "lib": [
          "dom", "es5", "es2015", "es2016", "es2017"
      ]
  }
}
